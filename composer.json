{
    "name": "nachokork/symfony-discord-monitor",
    "description": "This package is intended to monitor our symfony applications using Discord! The package will help us detect errors and related messages in your configured channel!",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.4 || ^8.2",
        "symfony/http-kernel": "^5.0 || ^6.0",
        "symfony/dependency-injection": "^5.0 || ^6.0",
        "symfony/config": "^5.0 || ^6.0",
        "symfony/yaml": "^5.0 || ^6.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "authors": [
        {
            "name": "Ignacio",
            "email": "nfernandez@quadram.mobi"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\ErrorBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
